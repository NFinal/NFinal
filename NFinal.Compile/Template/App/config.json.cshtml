{
  "urlPrefix": "/@{@Model.app}",
  "cookiePrefix": "@{@Model.app}_",
  "sessionPrefix": "@{@Model.app}_",
  "defaultStyle": "Default",
  "urlExtension": ".html",
  "urlMode": "1",
  "version": "1.0",
  "autoVersion": false,
  "compressHTML": true,
  "connectionStrings": [
    {
      "name": "Common",
      "connectionString": "Data Source=|ModuleDataDirectory|Common.db;Pooling=true;FailIfMissing=false",
      "providerName": "System.Data.SQLite"
    }
  ],
  "rewriteDirectory": [
  ],
  "rewriteFile": [
    {
      "from": "/@{@Model.app}/Index.html",
      "to": "/@{@Model.app}/IndexController/Index.html"
    }
  ],
  "redis": {
    "autoStart": true,
    "maxReadPoolSize": 60,
    "maxWritePoolSize": 60,
    "readWriteHosts": [ "127.0.0.1:6379" ],
    "readOnlyHosts": [ "127.0.0.1:6379" ]
  }
}