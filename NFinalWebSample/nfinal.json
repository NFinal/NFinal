  "version": "2.0",
  "config": {
    //用户自定义设置
    "appSettings": {
      "key": "value"
    },
    //连接字符串设置
    "connectionStrings": {
      "Common": {
        "connectionString": "Data Source=|ModuleDataDirectory|Common.db;Pooling=true;FailIfMissing=false",
        "providerName": "System.Data.SQLite"
      },
      "LeZhaiQuanStart": {
        "connectionString": "Server=192.168.1.102;Database=lezhaiquanstart;Uid=root;Pwd=root;",
        "providerName": "MySql.Data.MySqlClient"
      }
    },
    "verbs": [ "GET", "POST", "HEAD", "DEBUG" ],
    //Session设置
    "sessionState": {
      "cookieName": "session_id",
      "mode": "Redis",
      "stateConnectionString": "localhost",
      "timeout": 20,
      "prefix": ""
    },
    //Url设置
    "url": {
      "prefix": "",
      "extension": ".html",
      "defaultDocument": "index.html"
    },
    //Cookie设置
    "cookie": {
      "prefix": ""
    },
    //默认皮肤
    "defaultSkin": null,
    //用户自定义错误
    "customErrors": {
      "defaultRedirect": "Err.htm",
      "mode": "Off",
      "errors": [
        {
          "statusCode": 403,
          "redirect": "NoAccess.htm"
        },
        {
          "statusCode": 404,
          "redirect": "FileNotFound.htm"
        }
      ]
    }
  },
  //调试设置
  "debug": {
    "enable": true,
    "url": "http://localhost:55642",
    "directory": "/Debug"
  },
  //服务器设置
  "server": {
    "url": "http://www.domain.com",
    "indexDocument": "/Index.html"
  },
  //项目类型
  "projectType": "Console"
}